#!/bin/bash
export PROJECT_NAME=PowdrMap
export PROJECT_HOME=$HOME/external_projects/$PROJECT_NAME/$PROJECT_NAME
find $PROJECT_HOME -name '*.pyc'
rm `find $PROJECT_HOME -name '*.pyc'` -rf
find $PROJECT_HOME -name '#*#'
rm `find $PROJECT_HOME -name '#*#'` -rf
find $PROJECT_HOME -name '*~'
rm `find $PROJECT_HOME -name '*~'` -rf
